# Twilio Configuration for SMS OTP
# Sign up at https://www.twilio.com/ to get these credentials

# Your Twilio Account SID (found in Twilio Console)
TWILIO_ACCOUNT_SID=your_account_sid_here

# Your Twilio Auth Token (found in Twilio Console)
TWILIO_AUTH_TOKEN=your_auth_token_here

# Your Twilio Phone Number (must be verified in Twilio Console)
# Format: +1234567890
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Example values (replace with your actual Twilio credentials):
# TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# TWILIO_AUTH_TOKEN=your_auth_token_here
# TWILIO_PHONE_NUMBER=+11234567890